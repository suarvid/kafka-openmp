build simple-producer: c_LINKER simple-producer.p/simple_producer.c.o simple-producer.p/read_data.c.o | /usr/lib/x86_64-linux-gnu/librdkafka.so
 LINK_ARGS = -Wl,--as-needed -Wl,--no-undefined -Wl,--start-group /usr/lib/x86_64-linux-gnu/librdkafka.so -Wl,--end-group

# Test rules

build meson-test: CUSTOM_COMMAND all PHONY
 COMMAND = /usr/bin/meson test --no-rebuild --print-errorlogs
 DESC = Running$ all$ tests.
