ninja_required_version = 1.7.1

# Rules for compiling.

rule c_COMPILER
 command = cc $ARGS -MD -MQ $out -MF $DEPFILE -o $out -c $in
 deps = gcc
 depfile = $DEPFILE_UNQUOTED
 description = Compiling C object $out

# Rules for linking.

rule c_LINKER
 command = cc $ARGS -o $out $in $LINK_ARGS
 description = Linking target $out

# Other rules

rule CUSTOM_COMMAND
 command = $COMMAND
 description = $DESC
 restat = 1

rule REGENERATE_BUILD
 command = /usr/bin/meson --internal regenerate /home/arvid/Documents/exjobb/sandbox/test_producer /home/arvid/Documents/exjobb/sandbox/test_producer/build --backend ninja
 description = Regenerating build files.
 generator = 1

# Phony build target, always out of date

build PHONY: phony 

# Build rules for targets

build simple-producer.p/simple_producer.c.o: c_COMPILER ../simple_producer.c
 DEPFILE = simple-producer.p/simple_producer.c.o.d
 DEPFILE_UNQUOTED = simple-producer.p/simple_producer.c.o.d
 ARGS = -Isimple-producer.p -I. -I.. -fdiagnostics-color=always -pipe -D_FILE_OFFSET_BITS=64 -Wall -Winvalid-pch -g -Wall -Wextra -Ofast -g -Wl,--start-group -lrdkafka -Wl,--end-group

build simple-producer.p/read_data.c.o: c_COMPILER ../read_data.c
 DEPFILE = simple-producer.p/read_data.c.o.d
 DEPFILE_UNQUOTED = simple-producer.p/read_data.c.o.d
